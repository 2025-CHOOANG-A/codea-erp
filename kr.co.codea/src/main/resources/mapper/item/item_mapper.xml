<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.codea.item.item_mapper">

<!-- insert  -->
  <insert id="insert_item" parameterType="kr.co.codea.item.item_DTO">
    INSERT INTO ITEM (
        ITEM_ID,UNIT_CODE,ITEM_CODE,ITEM_NAME,ITEM_TYPE,ITEM_CAL_L,ITEM_CAL_S
    )
    VALUES (
        #{ITEM_ID},
        #{UNIT_CODE},
        #{ITEM_CODE}, 
        #{ITEM_NAME},
        #{ITEM_TYPE},
        #{ITEM_CAL_L},
        #{ITEM_CAL_S}
    )
</insert>

<select id="item_select" resultType="kr.co.codea.item.item_DTO">
    SELECT
      ITEM_ID,
      UNIT_CODE,
      ITEM_CODE,
      ITEM_NAME,
      ITEM_TYPE,
      ITEM_CAT_L,
      ITEM_CAT_S
    FROM ITEM
    ORDER BY ITEM_ID DESC

</select>


</mapper>